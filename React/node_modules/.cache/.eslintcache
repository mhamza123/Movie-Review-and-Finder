   }\n    } else {\n      setErrorMessages({ name: \"uname\", message: errors.uname });\n    }\n  };\n\n  const handleLogout = () => {\n    setIsSubmitted(false);\n    history.push('/login');\n  };\n\n  const renderErrorMessage = (name) =>\n    name === errorMessages.name && (\n      <div className=\"error\">{errorMessages.message}</div>\n    );\n\n  const renderForm = (\n    <div className=\"form\">\n      <form onSubmit={handleSubmit}>\n        <div className=\"input-container\">\n          <label>Username </label>\n          <input type=\"text\" name=\"uname\" required />\n          {renderErrorMessage(\"uname\")}\n        </div>\n        <div className=\"input-container\">\n          <label>Password </label>\n          <input type=\"password\" name=\"pass\" required />\n          {renderErrorMessage(\"pass\")}\n          <button className=\"submit\" type=\"submit\">Sign In</button>\n        </div>\n        <br />\n        <div className=\"have-account\">\n          <span>Don't have an account? </span>\n          <button className=\"signup\" onClick={() => history.push('/create-account')}>Sign Up</button>\n        </div>\n      </form>\n    </div>\n  );\n\n  return (\n    <div className=\"app\">\n      <div className=\"login-form\">\n        <div className=\"title\">Login</div>\n        {isSubmitted ? (\n          <div>\n            <div>User is successfully logged in</div>\n            <button onClick={handleLogout}>Logout</button>\n          </div>\n        ) : (\n          renderForm\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Login;\n","/home/tk-lpt-0602/Desktop/movie-app/movie-app/src/components/Home.js",[],[],"/home/tk-lpt-0602/Desktop/movie-app/movie-app/src/components/Browse.js",["79"],[],"/home/tk-lpt-0602/Desktop/movie-app/movie-app/src/components/useFetch.js",[],[],"/home/tk-lpt-0602/Desktop/movie-app/movie-app/src/components/AllMovies.js",[],[],"/home/tk-lpt-0602/Desktop/movie-app/movie-app/src/components/Movie.js",[],[],"/home/t